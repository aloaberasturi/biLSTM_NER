{ "arch": {
    "pre_trained": true,
    "w_embedding": 300,
    "c_embedding": 25,
    "lstm_char_units": 25,
    "lstm_main_units": 80,
    "dense_units": 160,
    "return_sequences": true,
    "mask_zero": true,
    "activation": "tanh"
  },

  "training": {
    "batch": 32,
    "dropout": 0.2,
    "rcrr_dropout": 0.5,
    "epochs": 50,
    "optimizer": "adam",
    "loss": "crf_loss",
    "metrics":"crf_accuracy"
  }
}