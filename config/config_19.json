{ "arch": {
    "pre_emb": true,
    "lower_emb": true,
    "w_embedding": 300,
    "c_embedding":25,
    "lstm_char_units": 25,
    "lstm_main_units": 150,
    "dense_units": 40,
    "return_sequences": true,
    "mask_zero": true,
    "activation": "relu"
  },

  "training": {
    "batch": 32,
    "dropout": 0.5,
    "epochs": 50,
    "optimizer": "adam",
    "loss": "loss_function",
    "metrics":"accuracy"
  }
}